{
  "name": "rsbuildEslintPrettier",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "build": "rsbuild build",
    "dev": "rsbuild dev --open",
    "preview": "rsbuild preview",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix ",
    "format": "prettier . --check --ignore-unknown",
    "format:fix": "prettier . --write \"src/*.{js,jsx,ts,tsx,json}\"",
    "component": "generate-react component",
    "page": "generate-react component  --type=page",
    "layout": "generate-react component  --type=layout",
    "redux": "generate-react component  --type=redux",
    "generate-sitemap": "node generate-sitemap.cjs",
    "prepare": "husky install",
    "init-husky": "npx husky-init",
    "test": "npx lint-staged"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.6.0",
    "@million/lint": "^1.0.0-rc.81",
    "@reduxjs/toolkit": "^2.2.5",
    "axios": "^1.7.2",
    "dayjs": "^1.11.11",
    "jwt-decode": "^4.0.0",
    "million": "^3.1.11",
    "react": "19.0.0-rc-01172397-20240716",
    "react-dom": "19.0.0-rc-01172397-20240716",
    "react-error-boundary": "^4.0.13",
    "react-hook-form": "^7.51.5",
    "react-redux": "^9.1.2",
    "react-router-dom": "^6.25.1",
    "sharp": "^0.33.4",
    "styled-components": "^6.1.12",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@rsbuild/core": "1.0.1-beta.3",
    "@rsbuild/plugin-babel": "^1.0.1-beta.3",
    "@rsbuild/plugin-image-compress": "^1.0.1",
    "@rsbuild/plugin-lightningcss": "^1.0.1-beta.3",
    "@rsbuild/plugin-react": "1.0.1-beta.3",
    "@rsbuild/plugin-styled-components": "^1.0.1-beta.3",
    "@rsbuild/plugin-svgr": "^1.0.1-beta.3",
    "@rsdoctor/rspack-plugin": "^0.3.8",
    "babel-plugin-react-compiler": "^0.0.0-experimental-696af53-20240625",
    "compression-webpack-plugin": "^11.1.0",
    "generate-react-cli": "^8.4.6",
    "husky": "^8.0.0",
    "lint-staged": "^15.2.7",
    "rsbuild-plugin-google-analytics": "^1.0.1",
    "rsbuild-plugin-open-graph": "^1.0.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "prettier": "^3.3.1",
    "@ianvs/prettier-plugin-sort-imports": "^4.3.1"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,html,ts,tsx,json,md}": [
      "eslint --fix",
      "prettier --write --ignore-unknown"
    ],
    "src/**/*.css": [
      "prettier --write --ignore-unknown"
    ]
  }
}
